{% if view == "index-slider" %}
	{% for value in list %}	
	<div class="{{prefix}}item  {{mod}}" style="background-image: url({{value.bg}})">
		<div class="{{prefix}}preview-wrap  {{mod}}">
			<div class="{{prefix}}preview  {{mod}}">
				<img src="{{value.img}}" class="img-fluid" alt="">
			</div>
		</div>
		<div class="{{prefix}}bg  {{mod}}" style="background-image: url({{value.bg_card}})"></div>
		<div class="{{prefix}}desc  {{mod}}">
			{% for t in value.text %}
				<p>{{t.name}}</p>
			{% endfor %}
		</div>
		<div class="{{prefix}}btn  {{mod}}">
			{% include "../button/btn.twig" with {
				view:'tpl',
				tag:'a',
				href:value.link,
				prefix:'btn__',
				mod:'is--white  is--lg',
				name: 'Узнать подробнее',
			} %}
		</div>
	</div>
	{% endfor %}
{% endif %}
{% if view == "catalog-banner" %}
	{% for value in list %}	
	<a href="{{value.link}}" class="{{prefix}}item  {{mod}}">
		<p class="{{prefix}}heading  {{mod}}" {% if value.color %}style="color: {{value.color}}"{% endif %}>{{value.name}}</p>
		<picture class="{{prefix}}preview  {{mod}}">
			<img src="{{value.img}}" class="img-fluid" alt="">
		</picture>
	</a>
	{% endfor %}
{% endif %}
